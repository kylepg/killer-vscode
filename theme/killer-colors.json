	"tokenColors": [
		{
			"name": "Comment",
			"scope": ["comment", "punctuation.definition.comment"],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#547180"
			}
		},
		{
			"name": "Constant",
			"scope": "constant",
			"settings": {
				"foreground": "#f76f67"
			}
		},
		{
			"name": "Entity",
			"scope": "entity",
			"settings": {
				"foreground": "#faaa4c"
			}
		},
		{
			"name": "Invalid",
			"scope": "invalid",
			"settings": {
				"foreground": "#ff342a9c"
			}
		},
		{
			"name": "Keyword",
			"scope": "keyword, storage.type.class.js",
			"settings": {
				"foreground": "#faaa4c"
			}
		},
		{
			"name": "Meta",
			"scope": "meta",
			"settings": {
				"foreground": "#9effff"
			}
		},
		{
			"name": "Meta Brace",
			"scope": "meta.brace",
			"settings": {
				"foreground": "#e1efff"
			}
		},
		{
			"name": "Punctuation",
			"scope": "punctuation",
			"settings": {
				"foreground": "#c4c1bb"
			}
		},
		{
			"name": "Punctuation Parameters",
			"scope": "punctuation.definition.parameters",
			"settings": {
				"foreground": "#e6e6e6"
			}
		},
		{
			"name": "Punctuation Template Expression",
			"scope": "punctuation.definition.template-expression",
			"settings": {
				"foreground": "#ffee80"
			}
		},
		{
			"name": "Storage",
			"scope": [
				"storage",
				"source.js punctuation.quasi.element.begin.js",
				"source.js punctuation.quasi.element.end.js"
			],
			"settings": {
				"foreground": "#faaa4c"
			}
		},
		{
			"name": "Storage Type Arrow Function",
			"scope": "storage.type.function.arrow",
			"settings": {
				"foreground": "#faaa4c"
			}
		},
		{
			"name": "String",
			"scope": ["string", "punctuation.definition.string"],
			"settings": {
				"foreground": "#81c784"
			}
		},
		{
			"name": "String Template",
			"scope": ["string.template", "punctuation.definition.string.template"],
			"settings": {
				"foreground": "#3ad900"
			}
		},
		{
			"name": "Support",
			"scope": "support",
			"settings": {
				"foreground": "#fff"
			}
		},
		{
			"name": "Support Function",
			"scope": "support.function",
			"settings": {
				"foreground": "#faaa4c"
			}
		},
		{
			"name": "Support Variable Property DOM",
			"scope": "support.variable.property.dom",
			"settings": {
				"foreground": "#e1efff"
			}
		},
		{
			"name": "Variable",
			"scope": ["variable", "punctuation.dollar.js"],
			"settings": {
				"foreground": "#e6e6e6"
			}
		},
		{
			"name": "[CSS] - Entity",
